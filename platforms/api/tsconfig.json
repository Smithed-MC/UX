		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": false,
		"jsx": "react-jsx",
		"outDir": "dist"
	},
	"ts-node": {
		"files": true
	},
	"files": ["src/type_definitions/abstract-cache.d.ts"]
}
